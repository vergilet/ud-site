window.series_<%= @series.id %> = '<%= escape_javascript(render partial: 'series/cover_ajax', locals: { cover: @series_presenter.cover, category_id: @series_presenter.category.id }) %>';
$(".series_<%= @series.id %>")
  .empty()
  .append(window.series_<%= @series.id %>)
  .find('.inside-image')
  .fadeIn( 1000 );
